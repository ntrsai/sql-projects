create database raju123;
use raju123;
create table student(id int primary key,
name varchar(40) not null,
age int not null);
desc student;
create table employee(id int primary key,
name varchar(20) not null,
salary float not null);
desc employee;
insert into student values(1,'sai',32);
insert into student values(2,'rajesh',52);
insert into student values(3,'bahi',12);
select * from student;
insert into employee values(1,'rohit',32);
insert into employee values(2,'rahul',34);
insert into employee values(3,'ram',31);
select * from employee union select * from student;
select * from employee except select * from student;
select * from employee intersect select * from student;





